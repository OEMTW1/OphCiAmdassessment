

table.subtleWhiteSmall { width:50%; background:#fff; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; margin-bottom:20px;}
table.subtleWhiteSmall tbody tr:hover { background-color: #fffbda; }
table.subtleWhiteSmall th { text-align:left; font-size:11px; color:#ccc; padding:10px 10px 2px 10px; }
table.subtleWhiteSmall td { font-size:15px; padding:10px; }
